CREATE TABLE SERVICE (SERVNO INTEGER GENERATED BY DEFAULT AS IDENTITY NOT NULL, LOCALISATION VARCHAR(45) NOT NULL, NOM VARCHAR(45) NOT NULL UNIQUE, MEDENO INTEGER, PRIMARY KEY (SERVNO))
CREATE TABLE MEDECIN (MEDENO INTEGER GENERATED BY DEFAULT AS IDENTITY NOT NULL, NOM VARCHAR(45) NOT NULL UNIQUE, PRENOM VARCHAR(45) NOT NULL, SALAIRE FLOAT, SERVNO INTEGER, PRIMARY KEY (MEDENO))
ALTER TABLE SERVICE ADD CONSTRAINT FK_SERVICE_MEDENO FOREIGN KEY (MEDENO) REFERENCES MEDECIN (MEDENO)
ALTER TABLE MEDECIN ADD CONSTRAINT FK_MEDECIN_SERVNO FOREIGN KEY (SERVNO) REFERENCES SERVICE (SERVNO)
